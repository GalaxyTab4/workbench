s,@USE_VFS_FALSE@,#,;t t
s,@USE_VFS_NET_TRUE@,,;t t
s,@USE_VFS_NET_FALSE@,#,;t t
s,@USE_UNDEL_FS_TRUE@,#,;t t
s,@USE_UNDEL_FS_FALSE@,,;t t
s,@USE_SAMBA_FS_TRUE@,#,;t t
s,@USE_SAMBA_FS_FALSE@,,;t t
